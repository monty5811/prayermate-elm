{
  "name": "elm-prayermate",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Dean Montgomery <montgomery.dean97@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.28.7",
    "elm": "^0.18.0",
    "elm-format": "^0.7.0-exp",
    "elm-test": "^0.18.9",
    "elm-webpack-loader": "^4.3.1",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "^1.7.2",
    "style-loader": "^0.20.0",
    "tailwindcss": "^0.5",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3",
    "workbox-webpack-plugin": "^3.0.1"
  },
  "scripts": {
    "watchelm": "find . -name '*.elm' | entr -c elm make src/Main.elm --warn --output /tmp/tmp.js",
    "watchelm-test": "find . -name '*.elm' | entr -c yarn test",
    "test": "elm-test tests/ --fuzz 100",
    "build": "webpack --mode production",
    "build:copy:redirects": "cp src/_redirects public/_redirects",
    "build:copy:json": "cp src/test_data.json public/test_data.json",
    "build:functions": "webpack --config webpack.config.functions.js --mode production",
    "dev": "WATCH=true webpack-dev-server --watch --color --progress --mode development",
    "generate_fixtures": "python tests/generate_fixtures.py",
    "format:elm": "elm-format --yes src tests",
    "format:js": "prettier  --single-quote --trailing-comma es5 --write \"src/**/*.js\"",
    "clean": "rm -rf public/ functions/ && mkdir public functions",
    "prebuild": "yarn clean && yarn build:copy:redirects && yarn build:copy:json",
    "predev": "yarn prebuild"
  }
}
